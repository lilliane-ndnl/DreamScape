{
  "expo": {
    "name": "DreamScape",
    "slug": "dreamscape",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/logo192.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logo192.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./public/favicon.ico"
    }
  }
} 